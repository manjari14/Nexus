{
  "version": 2,
  "builds": [
    {
      "src": "gweccc-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "gweccc-backend/index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/gweccc-backend/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/gweccc-backend/$1"
    }
  ]
}
