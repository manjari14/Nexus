{
    "version": 2,
    "projects": {
      "frontend": {
        "root": "frontend",
        "buildCommand": "npm install && npm run build",
        "outputDirectory": "build"
      },
      "backend": {
        "root": "backend",
        "buildCommand": "npm install",
        "outputDirectory": ""
      }
    },
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/backend/$1"
      }
    ]
  }
  