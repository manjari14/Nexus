{
  "version": 2,
  "builds": [
    {
      "src": "gweccc-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "gweccc-backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/gweccc-backend/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/gweccc-frontend/$1"
    }
  ]
}
